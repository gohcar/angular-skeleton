{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "removeComments": true,
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "sourceMap": false,
    "baseUrl": "codegen",
    "lib": ["es6", "dom"]
  },
  "exclude": [
    "node_modules",
    "src"
  ],
  "angularCompilerOptions": {
   "genDir": ".",
   "skipMetadataEmit" : true
 }
}